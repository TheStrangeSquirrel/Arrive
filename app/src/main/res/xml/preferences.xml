<PreferenceScreen xmlns:android="http://schemas.android.com/apk/res/android">
    <ListPreference
        android:defaultValue="1"
        android:entries="@array/entriesTypeArr"
        android:entryValues="@array/entryValuesTypeArr"
        android:key="pref_typeMap"
        android:title="@string/pref_typeMap" />
    <PreferenceCategory android:title="@string/pref_AlarmCategory">
        <RingtonePreference
            android:key="pref_ringtone"
            android:title="@string/pref_ringtone" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="pref_vibro"
            android:title="@string/pref_Vibro" />
        <CheckBoxPreference
            android:defaultValue="true"
            android:key="pref_loopPlay"
            android:title="@string/pref_loopPlay" />
    </PreferenceCategory>
</PreferenceScreen>